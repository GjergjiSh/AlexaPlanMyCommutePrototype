{
  "buildCommand": "pip install -r requirements.txt",
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}
